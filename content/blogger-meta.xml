<b:comment>[URL]______________________________________________________________________________________________________</b:comment>
<meta expr:content='data:view.url.canonical' property='og:url'/>
<link expr:href='data:view.url.canonical' rel='canonical'/>
<link expr:href='data:view.url.canonical appendParams {m: &quot;0&quot;}' rel='alternate'/>
<link expr:href='data:view.url.canonical appendParams {m: &quot;1&quot;}' rel='alternate'/>
<link expr:href='data:view.url.canonical appendParams {hl: &quot;en&quot;}' rel='alternate'/>

<meta expr:content='data:view.url.canonical' name='twitter:url'/>
<b:comment>[/URL]______________________________________________________________________________________________________</b:comment>

<b:comment>[Title]______________________________________________________________________________________________________</b:comment>
<b:if cond='data:view.isHomepage'><title><data:blog.title/></title></b:if>
<b:if cond='data:view.isMultipleItems'>
<b:if cond='data:view.search.query'><title><data:blog.title/> _ <data:messages.search/> | <data:view.search.query/></title></b:if>
<b:if cond='data:view.search.label'><title><data:blog.title/> | <data:blog.pageName/></title></b:if>
<b:if cond='data:view.isArchive'><title><data:blog.title/> _ <data:messages.archive/> | <data:blog.pageName/></title></b:if>
<b:if cond='data:view.search and !data:view.search.label and !data:view.search.query and !data:view.isArchive'><title><data:blog.title/></title></b:if>
<b:else/><title><data:blog.title/> | <data:blog.pageName/></title>
</b:if>

<meta expr:content='data:view.isSingleItem ? data:blog.title + &quot; | &quot; + data:blog.pageName : data:view.title' name='title'/>
<meta expr:content='data:view.isSingleItem ? data:blog.title + &quot; | &quot; + data:blog.pageName : data:view.title' property='og:title'/>
<meta expr:content='data:view.isSingleItem ? data:blog.title + &quot; | &quot; + data:blog.pageName : data:view.title' name='twitter:title'/>

<meta expr:content='data:blog.title' property='og:site_name'/>
<b:comment>[/Title]______________________________________________________________________________________________________</b:comment>

  
<b:comment>[Description]______________________________________________________________________________________________________</b:comment>
<meta expr:content='data:view.description' name='description'/>
<meta expr:content='data:view.description' property='og:description'/>
<meta expr:content='data:view.description' name='twitter:description'/>
<b:comment>[/Description]______________________________________________________________________________________________________</b:comment>

<b:comment>[images]______________________________________________________________________________________________________</b:comment>
<b:if cond='data:view.isSingleItem and data:view.featuredImage'>
	<link expr:href='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' rel='image_src'/>
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' name='thumbnail'/>
<b:elseif cond='data:view.isSingleItem and !data:view.featuredImage'/>
	<link expr:href='resizeImage(data:skin.vars.noPostImg , 1200 , &quot;1200:630&quot;)' rel='image_src'/>
	<meta expr:content='resizeImage(data:skin.vars.noPostImg , 1200 , &quot;1200:630&quot;)' name='thumbnail'/>
<b:elseif cond='data:view.isMultipleItems and data:view.featuredImage and !data:skin.vars.mainImg'/>
	<link expr:href='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' rel='image_src'/>  
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' name='thumbnail'/>
<b:elseif cond='data:view.isMultipleItems and data:skin.vars.mainImg'/>
	<link expr:href='resizeImage(data:skin.vars.mainImg , 1200 , &quot;1200:630&quot;)' rel='image_src'/>
	<meta expr:content='resizeImage(data:skin.vars.mainImg , 1200 , &quot;1200:630&quot;)' name='thumbnail'/>
</b:if>

<b:if cond='data:view.isSingleItem and data:view.featuredImage'>
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' property='og:image'/>
<b:elseif cond='data:view.isSingleItem and !data:view.featuredImage'/>
  <meta expr:content='resizeImage(data:skin.vars.noPostImg , 1200 , &quot;1200:630&quot;)' property='og:image'/>
<b:elseif cond='data:view.isMultipleItems and data:view.featuredImage and !data:skin.vars.mainImg'/>
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' property='og:image'/>
<b:elseif cond='data:view.isMultipleItems and data:skin.vars.mainImg'/>
  <meta expr:content='resizeImage(data:skin.vars.mainImg , 1200 , &quot;1200:630&quot;)' property='og:image'/>
</b:if>    
<meta content='1200' property='og:image:width'/>
<meta content='630' property='og:image:height'/>
<meta expr:content='data:view.title' property='og:image:alt'/>


<b:if cond='data:view.isSingleItem and data:view.featuredImage'>
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' name='twitter:image'/>
<b:elseif cond='data:view.isSingleItem and !data:view.featuredImage'/>
  <meta expr:content='resizeImage(data:skin.vars.noPostImg , 1200 , &quot;1200:630&quot;)' name='twitter:image'/>
<b:elseif cond='data:view.isMultipleItems and data:view.featuredImage and !data:skin.vars.mainImg'/>
	<meta expr:content='data:view.featuredImage.isYoutube ? resizeImage(data:view.featuredImage.youtubeMaxResDefaultUrl , 1200 , &quot;16:9&quot;) : resizeImage(data:view.featuredImage , 1200 , &quot;1200:630&quot;)' name='twitter:image'/>
<b:elseif cond='data:view.isMultipleItems and data:skin.vars.mainImg'/>
  <meta expr:content='resizeImage(data:skin.vars.mainImg , 1200 , &quot;1200:630&quot;)' name='twitter:image'/>
</b:if>
<meta content='1200' name='twitter:image:width'/>
<meta content='630' name='twitter:image:height'/>
<meta expr:content='data:view.title' name='twitter:image:alt'/>

<b:comment>[/images]______________________________________________________________________________________________________</b:comment>




<b:comment>[PWA]______________________________________________________________________________________________________</b:comment>
<link id='manifest-placeholder' rel='manifest'/>
<script>//<![CDATA[ 
document.querySelector('#manifest-placeholder').setAttribute('href', '/dynamic-manifest-url.json');
//]]>
const DynamicManifest = {
&quot;name&quot;: &quot;<data:blog.title.jsonEscaped/>&quot;,
&quot;short_name&quot;: &quot;<data:blog.title.jsonEscaped/>&quot;,
&quot;description&quot;: &quot;<data:view.description/>&quot;,
&quot;start_url&quot;: &quot;<b:eval expr='data:blog.homepageUrl.canonical appendParams {source: &quot;pwa&quot;}'/>&quot;,
&quot;scope&quot;: &quot;<data:blog.homepageUrl.canonical/>&quot;,
&quot;dir&quot;: &quot;<data:blog.languageDirection/>&quot;,
&quot;orientation&quot;: &quot;any&quot;,
&quot;display&quot;: &quot;standalone&quot;,
&quot;background_color&quot;: &quot;#fafafa&quot;,
&quot;theme_color&quot;: &quot;<data:skin.vars.primary/>&quot;,
&quot;icons&quot;: [
{ &quot;src&quot;: &quot;<data:blog.blogspotFaviconUrl/>&quot;, &quot;type&quot;: &quot;image/x-icon&quot; },
{ &quot;src&quot;: &quot;<b:eval expr='resizeImage(data:skin.vars.logo, 192)'/>&quot;, &quot;type&quot;: &quot;image/png&quot;, &quot;sizes&quot;: &quot;192x92&quot;},
{ &quot;src&quot;: &quot;<b:eval expr='resizeImage(data:skin.vars.logo, 512)'/>&quot;, &quot;type&quot;: &quot;image/png&quot;, &quot;sizes&quot;: &quot;512x512&quot;}
]
}
//<![CDATA[
const stringManifest = JSON.stringify(DynamicManifest); const blob = new Blob([stringManifest], {type: 'application/json'});
const manifestURL = URL.createObjectURL(blob); document.querySelector('#manifest-placeholder').setAttribute('href', manifestURL);
//]]></script>

